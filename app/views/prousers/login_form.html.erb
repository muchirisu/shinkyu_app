<div class="main prousers-new">
  <div class="container">
    <div class="form-head">ログイン</div>
      <div class="form prousers-form">
        <div class="form-body">
          <% if @error_message %>
            <div class="form-error">
              <%= @error_message %>
            </div>
          <% end %>
          <%= form_tag("/prousers/login") do %>
              <p>メールアドレス</p>
              <input name="email" velue="<%= @email %>">
              <p>パスワード</p>
              <input type="password" name="password">
              <input type="submit" value="ログイン">
          <% end %>
        </div>
      </div>
  </div>
</div>